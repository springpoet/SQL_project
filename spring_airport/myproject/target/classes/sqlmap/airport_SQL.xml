	]]>
	</insert>
	 -->
	  
	<insert id="sing_up" parameterType="com.myproject.dto.MemberDto" >
	<![CDATA[
	insert into member values (#{id}, #{pw}, #{name}, #{gender}, #{age})
	]]>
	</insert> 
</mapper>